{
    "version": 2,
    "builds": [
        {
            "src": "api/tweetbuzz.js",
            "use": "@vercel/node"
        },
        {
            "src": "index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/tweetbuzz",
            "dest": "/api/tweetbuzz.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}